    	function updateVal(currentEle, value, value2, indicatorID, field, oldValue) {
            $(currentEle).html('<textarea id="newEdit" indicatorID ="' + indicatorID.value + 
                                    '" field="' + field.value +
                                    '" value="' + value + 
                                    '" class="thVal pr-edit-textarea" rows="1">' + value +
                                    '</textarea>' +
                                '<textarea id="newEdit" indicatorID ="' + indicatorID.value + 
                                    '" field="' + field.value +
                                    '" value="' + value2 + 
                                    '" class="thVal2 pr-edit-textarea" rows="1">' + value2 +
                                    '</textarea>' +
                                '<textarea id="newEdit" indicatorID ="' + indicatorID.value + 
                                    '" value="' + value2 + 
                                    '" class="thFootnotes pr-edit-textarea" rows="1">' + value2 +
                                    '</textarea>'
                                );
            if (field.value === "Indicator"){
                $('.thVal2').hide();
                $('.thFootnotes').show();
                $('.thButton').show();
            } else {
                $('.thVal2').show();
                $('.thFootnotes').hide();
                $('.thButton').hide();
            }
            $(".thVal").focus();
            $(".thVal").keyup(function (event) {
                if (event.keyCode == 13) {
                }
            });

            $('body').unbind("click").click(function (evt) {
            	endEdit(evt, currentEle);
            });
            $(document).on('keypress', '.pr-edit-textarea:not(.thVal-Def):not(.thVal-Obj)', function (evt) {
                if (evt.which == 13) {
                    var evt = {target: "none"};
                    endEdit(evt, currentEle);
                };
            });
            if(oldValue === "%UNDO%") {endEdit(null, currentEle);}

            endEdit = function(evt, currentEle){
            	if (evt !== null){
            		if (evt.target.id == "newEdit" || evt.target.id == "newEdit2" || evt.target.id == "editFootnotes") return;
            	}
            	var colorElement = $(currentEle);
                var prop = $('.thVal').attr('field');
                var parameters = {
                    indicatorID: $('.thVal').attr('indicatorID'),
                    updateProperty: prop,
