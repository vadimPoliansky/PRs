                        		if (item === startField){
                        			startFieldIndex = index;
                        		}
                        	});
                        	var parameters = { indicatorID: indicatorID, startField: startField, allFields: allFields}
                        	$.ajax({
                        		type:"POST",
                        		url: "unmergeCell",
                        		data: parameters,
                        		traditional: true,
                        	});
                        	for(i = oldColspan - 1; i > 0; i--){
