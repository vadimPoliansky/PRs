@{
    ViewBag.Title = "addFiscalYear";
}
<script src="~/Scripts/jquery-1.11.0.min.js"></script>
<div id="warning"><h2>Generate New Fiscal Year (Not Undoable!)</h2><p>Are you sure you want to create a new Fiscal Year? This step is Not Undoable! </p></div>

<div id="warning2">
	<button id="generate">Generate New Fiscal Year</button>
	<button id="cancel">Cancel</button>
</div>

<style>

	#warning, #warning2 {
		text-align:center;
	}

	body {
		background-color:red;
	}

</style>

<script>

	$('#generate').on('click', function (e) {
		if (window.confirm("Are you sure you want to generate a new fiscal year? This is NOT UNDOABLE!")) {
			location = "addFiscalYearCmd";
		}
	});

	$('#cancel').on('click', function (e) {
			window.close();
	});
</script>